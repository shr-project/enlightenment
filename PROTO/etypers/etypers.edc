   }
   group { name: "elm/entry/base-noedit/etypers";
      inherit: "elm/entry/base/etypers";
      parts {
         part { name: "elm.text";
            entry_mode: PLAIN;
            description { state: "default" 0.0;
               fixed: 1 0;
               text {
                  style: "entry_textblock_style";
                  min: 0 1;
                  align: 0.0 0.0;
               }
            }
            description { state: "disabled" 0.0;
               inherit: "default" 0.0;
               text {
                  style: "entry_textblock_disabled_style";
               }
            }
         }
      }
   }
